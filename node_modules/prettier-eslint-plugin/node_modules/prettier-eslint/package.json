{
  "name": "prettier-eslint",
  "version": "4.4.0",
  "description": "Formats your JavaScript using prettier followed by eslint --fix",
  "main": "dist/index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "common-tags": "^1.4.0",
    "dlv": "^1.1.0",
    "eslint": "^3.17.0",
    "indent-string": "^3.1.0",
    "lodash.merge": "^4.6.0",
    "loglevel-colored-level-prefix": "^1.0.0",
    "prettier": "^0.22.0",
    "pretty-format": "^19.0.0",
    "require-relative": "^0.8.7"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.0",
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint-config-kentcdodds": "^12.0.0",
    "husky": "^0.13.1",
    "jest-cli": "^19.0.1",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.3",
    "prettier-eslint-cli": "^3.0.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "strip-indent": "^2.0.0",
    "validate-commit-msg": "^2.8.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ],
    "rules": {
      "valid-jsdoc": 0,
      "max-len": [
        2,
        80
      ]
    }
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prettier/prettier-eslint.git"
  },
  "bugs": {
    "url": "https://github.com/prettier/prettier-eslint/issues"
  },
  "homepage": "https://github.com/prettier/prettier-eslint#readme"
}